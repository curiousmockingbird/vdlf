{
  "name": "sage",
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "engines": {
    "node": ">=12.14.0"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider mix",
    "build:production": "mix --production",
    "start": "mix watch",
    "hot": "mix watch --hot",
    "clear": "wp acorn optimize:clear",
    "test": "npm run lint",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "translate": "npm run translate:pot && npm run translate:js",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --ignore-domain --include=\"app,resources\"",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@tailwindcss/typography": "^0.4.1",
    "@wordpress/babel-preset-default": "^6.1.0",
    "@wordpress/browserslist-config": "^4.0.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.2.1",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "laravel-mix": "^6.0.49",
    "laravel-mix-svg-vue": "^0.3.7",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.3.0",
    "sass": "^1.34.0",
    "sass-loader": "11.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "svg-vue3": "^0.2.1",
    "tailwindcss": "^2.2.4",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/preset-react": "^7.24.7",
    "@vue-hero-icons/outline": "^1.7.2",
    "@vuex-orm/core": "^0.36.3",
    "axios": "^0.21.4",
    "crypto-js": "^4.1.1",
    "es6-promise": "^4.2.8",
    "gsap": "^3.6.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "nodemailer": "^6.10.0",
    "popper.js": "^1.16.1",
    "svg-vue": "^0.1.2",
    "vue": "^2.6.14",
    "vue-js-modal": "^2.0.1",
    "vue-loader": "^15.9.6",
    "vue-loading-overlay": "^3.4.2",
    "vue-resource": "^1.5.1",
    "vue-select": "^3.13.0",
    "vue-slick-carousel": "^1.0.6",
    "vue-social-sharing": "^3.0.8",
    "vuex": "^3.6.2"
  }
}
